.m-pagination {
    display: block;
    width: 1000px;
    text-align:center;
    margin 40px auto 60px;
}
.m-pagination a {
    display: inline-block;
    width: 40px;
    height: 40px;
    line-height: 40px;
    text-align: center;
    color: #78829d;
    font-size: 14px;
    background: #f6f6f6;
    border-bottom: 1px solid #cecece;
    margin: 0 1px;
}
.m-pagination a.page-prev, .m-pagination a.page-next {
    color: #e9eef9;
    background: #b6c8eb;
    font-size: 14px;
}
.m-pagination a:hover {
    background: #5882e2;
    color: #fff;
}
.m-pagination a.page-cur {
    color: #f60;
    font-weight: bold;
}
.m-pagination a.page-cur:hover {
    background: #f6f6f6;
    color: #f60;
    cursor: default;
}
.m-pagination .disabled {
    cursor: not-allowed;
}
.m-pagination .page-right10 {
    padding-right: 15px;
}
.m-pagination .page-left10 {
    padding-left: 15px;
}
.m-pagination label {
    display: inline-block;
    line-height: 40px;
    text-align: center;
    color: #78829d;
    font-size: 14px;
}
.m-pagination .page-number {
    width: 30px;
    text-align: center;
    line-height: 20px;
    height: 20px;
    background: #fff;
    border: 1px solid #cecece;
    color: #78829d;
