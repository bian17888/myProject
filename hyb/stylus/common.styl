/*  === 轮转模块 c-slide-xx ===  */
.c-slide { position: relative; height:515px; overflow: hidden; z-index: 9; }
.c-slide .c-slide-cont li { position: relative; vertical-align: middle; z-index: 9; }
.c-slide .c-slide-pages { position: absolute; left: 0; bottom: 10px; width: 1000px; height: 20px; overflow: hidden; z-index: 99; }
.c-slide .c-slide-pages .txC { text-align:center;}
.c-slide .c-slide-pages a { display: inline-block; width: 15px; height: 15px; line-height: 9999px; text-align: center; background-image: none; background-color: #fff; opacity:0.5; filter: unquote('alpha(Opacity=50)'); margin: 0 5px; cursor: pointer; -webkit-border-radius: 50%; -moz-border-radius: 50%; border-radius: 50%; }
.c-slide .c-slide-pages a:hover, .c-slide .c-slide-pages a.active { background-color: #fff; opacity:1;filter: unquote('alpha(Opacity=100)'); }
/*  reset c-slide-main  */
.c-slide-main { width: 100%; height: auto; overflow: hidden; margin: 0px 0 0; }
.c-slide-main .c-slide-cont li { width:100%; }
.c-slide-main .c-slide-cont li .li-box { min-width:1000px; width:100%; height:auto; margin:0 auto;}
.c-slide-main .c-slide-cont li .li-box img { display:block; width:100%;  }
.c-slide-main .c-slide-pages { left:50%; margin-left:-500px;}

